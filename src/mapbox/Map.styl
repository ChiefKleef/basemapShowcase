#map {
  postion: absolute;
  height: 100vh;
  left: 436px;
  width: calc(100vw - 436px);
}
.mapboxgl-popup-content {
  padding: 5px;
  .mapboxgl-popup-close-button{
    z-index: 100;
  }
  .selection {
    position: relative;
    display: block;
    .selectionTitle-container{
      width: 100%;
      position: relative;
      .selectionTitle {
        margin-left: 3px;
        position: relative;
        vertical-align: middle;
        line-height: 20px;
        font-weight: 600;
        font-size: 10pt;
        color: rgb(10, 10, 10);
        .selectionSubtitle {
          font-weight: 200;
          font-size: 7pt;
          color: rgb(100, 100, 100);
        }
      }
      .fa-square{
        margin-left: 5px;
      }
    }
    .selectionTitle-underline{
      height: 1px;
      width: calc(100% - 6px);
      position: relative;
      left: 3px;
      background-color: rgb(150,150,150);
      margin-bottom: 10px;
    }
    .propDisplayContainer{
     padding-top: 3px;
     padding-left: 6px;
     padding-bottom: 6px;
     padding-right: 6px;
     .selectionProps{
       width: 100%;
      .selectionProps-row{
        width: 100%;
        .propIcon{
          margin-left: 6px;
          // color: #236FA5;
          color: rgb(90,90,90);
        }
        .propDisplay-text{
            
        }
        .propDisplay-label{
          font-weight: bold;
          color: rgb(90,90,90);
        }
        .propDisplay-value{

        }
      }
     }
    }
    .tableContainer{
      height: 75px;
      overflow-y: scroll;
      border: 1px solid rgb(220,220,220);
      tr{
        font-size: 8.5pt;
        td{
          padding: 2px;
          height: 25px;
          max-width: 130px;
          .span-property{
            font-weight: 500;
            color: rgb(50,50,50);
          }
          .span-value{
            font-weight: 200;
            color: rgb(35,35,35);
          }
        }
        .td-property{
          border-right-width: .5px;
          border-right-style: solid;
          border-right-color: rgb(220,220,220);
          line-height: 16px;
        }
        .td-propert-long{

        }
        .td-value{
          border-right-width: .5px;
          border-right-style: solid;
          border-right-color: rgb(230,230,230);
          padding-left: 5px;
          max-width: 120px;
        }
      }
      .tr-dark{
        background-color: rgba(240,240,240,.9); 
      }
      #tr-final{
        td{
          border-bottom-width: 0px;
        }
      }
    }
  }
  .paneSpacerHorizontal {
    height: 5px;
  }
}