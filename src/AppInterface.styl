.AppInterface{
  position: absolute;
  z-index: 100;
  width: 100vw;
  height: 100vh;
  pointer-events: none;
  .title{
    position: absolute;
    background-color: rgba(255,255,255,.8)
    border-radius: 4px;
    font-family: 'Montserrat', sans-serif;
    font-weight: 900;
    color: black;
    padding: 3px;
    top: 10px;
    left: 10px;
    pointer-events:auto;
    box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.35);
  }
  .switchPanes{
    position: absolute;
    width: 300px;
    bottom: 10px;
    left: 10px;
  }
  .selectionPane{
    position: absolute;
    top: 10px;
    right: 50px;
  }
}