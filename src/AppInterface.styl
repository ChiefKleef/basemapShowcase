.AppInterface{
  position: absolute;
  z-index: 100;
  width: 100vw;
  height: 100vh;
  pointer-events: none;
  .title{
    position: absolute;
    background-color: rgba(255,255,255,.8)
    border-radius: 4px;
    font-family: 'Montserrat', sans-serif;
    font-weight: 900;
    padding: 3px;
    top: 10px;
    left: 10px;
    pointer-events:auto;
  }
}