.mapsPane{
  overflow-y: auto;
  .mapsPane-choices-titleContainer {
    height: 40px;
    width: calc(100% - 10px);
    box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.35);
    margin: 5px;
    text-align: center;
    line-height: 40px;
    font-size: 14pt;
    color: white;
    background-color: rgba(10, 131, 201, 1);
    .mapsPane-choices-titleIcon {
      margin-left: 10px;
    }
  }
  .mapsPane-buttons{
    display: flex!important;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: stretch;
    .mapsPane-button{
      z-index: 250;
      position: relative;
      height: 75px;
      text-align: center;
      cursor: pointer;
      line-height: 75px;
      transition-duration: .2s;
      user-select: none;
      a{
        transition-duration: .2s;
      }
      margin-bottom: 5px;
      .mapsPane-button-title{
        // color: rgb(160,160,160);
        font-size: 13.5pt;
        position: relative;
        z-index: 301;
        .mapsPane-button-icon{
          // color: rgb(160,160,160);
          margin-right: 10px;
        }
      }
    }

    .mapsPane-button-odd {
      margin-left: 5px;
      margin-right: 2.5px;
    }
    .mapsPane-button-even {
      margin-right: 5px;
      margin-left: 2.5px;
    }

    .mapsPane-loadButton {
      width: calc(50% - 7.5px);
    }
    
    .mapsPane-loadButton-clicked{
      box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.45);
      background-color: rgb(250,250,250);
      color: rgb(80,80,80);
      border-color: rgb(80,80,80);
      span{
        color: rgb(80,80,80);
      }
      a{
        color: rgb(80,80,80);
      }
    }
    .mapsPane-loadButton-notClicked{
      background-color: white;
      box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.45);
      color: rgb(160,160,160);
      a {
        color: rgb(160,160,160);
      }
      &:hover{
        color: rgb(80,80,80);
        a{
          color: rgb(80,80,80);
        }
        border-color: rgb(80,80,80);
        box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.45);
      }
    }

    .mapsPane-controlButton{
      width: calc(50% - 7.5px);
    }
    .mapsPane-controlButton-clicked{
      box-shadow: 0px 0px 1px rgba(0, 0, 0, 0.45);
      background-color: rgb(250,250,250);
      color: rgb(80,80,80);
      border-color: rgb(80,80,80);
      a{
        color: rgb(80,80,80);
      }
      .mapsPane-controlButton-uploadMask{
        background-color: rgb(250,250,250);
      }
    }
    .mapsPane-controlButton-notClicked{
      background-color: white;
      box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.45);
      color: rgb(160,160,160);
      a {
        color: rgb(160,160,160);
      }
      .mapsPane-controlButton-uploadMask{
        background-color: white;
      }
      &:hover{
        color: rgb(80,80,80);
        a{
          color: rgb(80,80,80);
        }
        border-color: rgb(80,80,80);
        box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.45);
      }
    }

    .mapsPane-controlButton-1{
      border: 1px solid rgb(180,180,180);
    }
    .mapsPane-controlButton-2{
      border: 1px dashed rgb(160,160,160);
    }
    .mapsPane-controlButton{
      .mapsPane-controlButton-uploadMask{
        position: absolute;
        top: 0px;
        left: 0px;
        width: 100%;
        height: 100%;
        z-index: 300;
      }
      input{
        position: absolute;
        top: 0px;
        left: 0px;
        width: 100%;
        height: 100%;
      }
    }
    .mapsPane-buttonRow-preceeding {
      
    }
  }
}