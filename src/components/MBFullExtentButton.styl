.mBFullExtentButton{
  background-color: white;
  border-radius: 5px;
  height: 30px;
  width: 30px;
  cursor: pointer;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.35);
  z-index: 100;
  &:hover{
    background-color: #F3F3F3;
  }
  .fa-expand{
    position: relative;
    left: 6.5px;
    top: 6px;
    pointer-events: none;
    font-size: 13.5pt;
    color: #333333;
  }
}