.loadingPane-active {
  opacity: 1;
  pointer-events: auto;
}
.loadingPane-inactive {
  opacity: 0;
  pointer-events: none;
}
.loadingPane {
  position: absolute;
  bottom: 0px;
  user-select: none;
  height: 100%;
  h = @height;
  width: 100%;
  left: 0px;
  background-image: linear-gradient(135deg, #34abfa, #585CF4);
  z-index: 1000000;
  transition: bottom .3s ease-in, opacity 0.3s linear;
  overflow: hidden;
  .loadingBar-container {
    position: absolute;
    display: flex;
    justify-content: center;
    align-content: center;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
  }
  .loadingBar {
    position: relative
    height: 30%;
    width: 3px;
    background-color: white;
    animation: upDown .8s infinite linear;
  }
  .loadingBar-backdrop {
    position: relative
    height: 100%;
    width: 3px;
    background-color: rgba(255,255,255,.3);
  }
}

  @-webkit-keyframes upDown {
    0% {
      -webkit-transform: translate3d(0px, 0px, 0px);
    }
    50% {
      -webkit-transform: translate3d(0px, 70vh, 0px);
    }
    100% {
      -webkit-transform: translate3d(0px, 0px, 0px);
    }
  }

  @keyframes upDown {
    0% {
      transform: translate3d(0px, 0px, 0px);
    }
    50% {
      transform: translate3d(0px, 70vh, 0px);
    }
    100% {
      transform: translate3d(0px, 0px, 0px);
    }
  }