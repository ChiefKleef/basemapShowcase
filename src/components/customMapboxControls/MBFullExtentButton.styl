.mBFullExtentButton {
  position: absolute;
  bottom: 150px;
  right: 10px;
  pointer-events: auto;
  background-color: white;
  border-radius: 5px;
  height: 30px;
  width: 30px;
  cursor: pointer;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.35);
  z-index: 100;
  text-align: center;
  vertical-align: middle;
  line-height: 33.5px;
  &:hover {
    background-color: #F3F3F3;
  }
  .fa-arrows-alt {
    position: relative;
    // left: 6.5px;
    // top: 6px;
    pointer-events: none;
    font-size: 13.5pt;
    color: #333333;
  }
  .alertFullExtent {
    position: absolute;
    right: 40px;
    line-height: 30px;
    text-shadow: 0px 0px 1px rgba(0, 0, 0, 1);
    color: yellow;
    font-size: 9pt;
    z-index: 1000;
    white-space: nowrap;
    user-select: none;
  }
}