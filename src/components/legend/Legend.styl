.legend{
  position: absolute;
  top: 0px;
  right:0px;
  margin: 10px;
  min-width: 200px;
  max-width: 300px;
  background-color: white;
  box-shadow: 0px 0px 5px rgba(0, 0, 0, 0.35);
  pointer-events: auto;
  .legend-titleBar{
    padding: 4px;
    background-color: #0A83C9;
    height: 28px;
    .legend-title{
      color: white;
      font-size: 12pt;
      width: 100%;
      .fa-list-ul{
        float: right;
        font-weight: 200;
        margin-top: 3px;
      }
    }
  }
  .legend-content{
    padding: 4px;
  }
}