.selectionGraphs {
  position: relative;
  height: 250;
  width: 90%;
  left: 5%;
  z-index:100
  .chartTitleBar {
    position: relative;
    width: 100%
    height: 25px;
    border-style: solid;
    border-bottom-width: 1px;
    border-bottom-color: rgb(200, 200, 200);
    z-index: 100;
    .chartTitle {
      height: inherit;
      font-family: 'Roboto', sans-serif;
      font-weight: 400;
      position: relative;
      color: rgb(40, 40, 40);
      bottom: 0px;
      right: 0px;
      float: right;
      padding-top: 2px;
      padding-left: 4px;
      padding-right: 4px;
      padding-bottom: 0px;
      border-style: solid;
      border-width: 1px;
      border-color: rgb(200, 200, 200);
      border-bottom-color: white;
    }
  }
  .rd3-chart{
    z-index: 101;
    font-size: 7pt;
  }
}