#appInterface-container {
  position: absolute;
  top: 0px;
  left: 0px;
  height: 100vh;
  width: 100vw;
}

.mainLoadingPane {
  position: absolute;
  pointer-events: auto;
  user-select: none;
  height: 100vh;
  width: 100vw;
  bottom: 0px;
  left: 0px;
  background-image: linear-gradient(135deg, #34abfa, #585CF4);
  z-index: 1000000;
  transition-duration: .5s;
  overflow: hidden;
  .loadingBar-container {
    position: absolute;
    display: flex;
    justify-content: center;
    align-content: center;
    height: 100%;
    width: 100%;
    top: 0px;
    left: 0px;
  }
  .loadingBar {
    position: relative
    height: 30%;
    width: 3px;
    background-color: white;
    animation: upDown .8s infinite linear;
  }
  .loadingBar-backdrop {
    position: relative
    height: 100%;
    width: 3px;
    background-color: rgba(255,255,255,.2);
  }
}

@-webkit-keyframes upDown {
  0% {
    -webkit-transform: translate3d(0px, 0px, 0px);
  }
  50% {
    -webkit-transform: translate3d(0px, 70vh, 0px);
  }
  100% {
    -webkit-transform: translate3d(0px, 0px, 0px);
  }
}

@keyframes upDown {
  0% {
    transform: translate3d(0px, 0px, 0px);
  }
  50% {
    transform: translate3d(0px, 70vh, 0px);
  }
  100% {
    transform: translate3d(0px, 0px, 0px);
  }
}
